{
  "name": "cocori-library",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --port 5050",
    "start": "ng serve --configuration 'local' --port 5050",
    "build": "ng build",
    "cypress": "npx cypress open",
    "lib": "ng build --project=cocori-ng --watch",
    "test": "ng test --project=cocori-ng --watch",
    "lib:build": "ng build --project=cocori-ng",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/cocori-library/stats.json",
    "link": "cd dist\\cocori-ng && npm link",
    "testproj": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run cocori-library:serve-ssr",
    "serve:ssr": "node dist/cocori-library/server/main.js",
    "build:ssr": "ng build --configuration production && ng run cocori-library:server:production",
    "prerender": "ng run cocori-library:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/cdk": "^13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.0.3",
    "@angular/material": "^13.0.3",
    "@angular/material-luxon-adapter": "^13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/platform-server": "~13.0.3",
    "@angular/router": "~13.0.3",
    "@nguniversal/express-engine": "^13.0.1",
    "@r-tek/colr_pickr": "^2.0.0",
    "@tinymce/tinymce-angular": "^4.2.4",
    "express": "^4.15.2",
    "luxon": "^2.0.2",
    "material-design-icons": "^3.0.1",
    "rxjs": "~7.4.0",
    "spark-md5": "^3.0.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.3",
    "@angular/language-service": "~13.0.3",
    "@types/express": "^4.17.0",
    "@types/faker": "^5.1.7",
