{
  "name": "cocori-library",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --port 5050",
    "start": "ng serve --configuration 'local' --port 5050",
    "build": "ng build",
    "cypress": "npx cypress open",
    "lib": "ng build --project=cocori-ng --watch",
    "test": "ng test --project=cocori-ng --watch",
    "lib:build": "ng build --project=cocori-ng",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/cocori-library/stats.json",
    "link": "cd dist\\cocori-ng && npm link",
    "testproj": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run cocori-library:serve-ssr",
    "serve:ssr": "node dist/cocori-library/server/main.js",
    "build:ssr": "ng build --configuration production && ng run cocori-library:server:production",
    "prerender": "ng run cocori-library:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/cdk": "^12.2.10",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.0.3",
    "@angular/material": "^12.2.10",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/platform-server": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@nguniversal/express-engine": "^11.2.1",
    "@r-tek/colr_pickr": "^2.0.0",
    "@tinymce/tinymce-angular": "^4.2.2",
    "express": "^4.15.2",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.6.0",
    "spark-md5": "^3.0.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.10",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.0.3",
    "@angular/language-service": "~12.0.3",
    "@types/express": "^4.17.0",
    "@types/faker": "^5.1.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/resize-observer-browser": "^0.1.5",
    "codelyzer": "^6.0.0",
    "cypress": "^6.0.1",
    "faker": "^5.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
